{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "pipeline": {
    "prepare": {
      "cache": false,
      "dependsOn": ["^prepare"]
    },
    "build": {
      "dependsOn": ["clean", "^prepare", "^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": [
        "NEXT_PUBLIC_ALCHEMY_ID",
        "NEXT_PUBLIC_BOT_DAO",
        "NEXT_PUBLIC_RPC_URL",
        "BASEURL",
        "NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID",
        "TELEGRAM_BOT_KEY",
        "QSTASH_URL",
        "NODE_ENV",
        "NEXT_PUBLIC_GETME_BOT_INFO",
        "DISABLE_AUDIT_LOGS",
        "NEXT_PUBLIC_CHAINNAME",
        "NEXT_PUBLIC_CHAINRESTURL",
        "NEXT_PUBLIC_NETWORK",
        "NEXT_PUBLIC_CHAINID",
        "MAINTENANCE_MODE",
        "CRON_SECRET",
        "QSTASH_TOKEN",
        "QSTASH_CURRENT_SIGNING_KEY",
        "QSTASH_NEXT_SIGNING_KEY",
        "ALLOWED_DENOMS"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["clean", "prepare"],
      "env": [
        "NEXT_PUBLIC_ALCHEMY_ID",
        "NEXT_PUBLIC_BOT_DAO",
        "NEXT_PUBLIC_RPC_URL",
        "PORT",
        "BASEURL",
        "NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID",
        "TELEGRAM_BOT_KEY",
        "QSTASH_URL",
        "NODE_ENV",
        "NEXT_PUBLIC_GETME_BOT_INFO",
        "DISABLE_AUDIT_LOGS",
        "NEXT_PUBLIC_CHAINNAME",
        "NEXT_PUBLIC_CHAINRESTURL",
        "NEXT_PUBLIC_NETWORK",
        "NEXT_PUBLIC_CHAINID",
        "MAINTENANCE_MODE",
        "CRON_SECRET",
        "QSTASH_TOKEN",
        "QSTASH_CURRENT_SIGNING_KEY",
        "QSTASH_NEXT_SIGNING_KEY",
        "ALLOWED_DENOMS"
      ],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  },
  "globalEnv": []
}
