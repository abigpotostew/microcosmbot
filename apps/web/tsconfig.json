{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "svg-module.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "components/*": ["components/*"],
      "libs/*": ["libs/*"],
      "constants/*": ["constants/*"],
      "styles/*": ["styles/*"],
      "assets/*": ["public/*"],
      "utils/*": ["utils/*"],
      "state/*": ["state/*"],
    }
  }
}
